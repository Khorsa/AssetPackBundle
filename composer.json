{
  "name": "flexycms/assetpack-bundle",
  "version": "0.1.0",
  "type": "symfony-bundle",
  "description": "Symfony bundles tutorial example project",
  "license": "proprietary",
  "require": {
    "php": "^7.3",
    "scssphp/scssphp": "^1.4"
  },
  "require-dev": {
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "flexycms\\AssetPackBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flexycms\\AssetPackBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test" : "./vendor/bin/simple-phpunit"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.0.*"
    }
  }
}